{
  "name": "vite-plugin-version-log",
  "version": "0.0.1",
  "description": "A Vite plugin to inject version information into HTML",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "dev:example": "concurrently \"pnpm preview:example\" \"pnpm dev:rebuild\"",
    "dev:rebuild": "tsup --watch --onSuccess \"node ./scripts/rebuild-example.js\"",
    "preview:example": "pnpm --filter example build && pnpm --filter example preview",
    "build": "tsup",
    "test": "vitest"
  },
  "keywords": [
    "vite-plugin",
    "version",
    "git"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/runner": "^1.0.0",
    "typescript": "^5.0.0",
    "concurrently": "^8.0.0",
    "ws": "^8.16.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  }
} 